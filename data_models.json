{
  "User": {
    "id": "string (uuid)",
    "email": "string",
    "created_at": "datetime",
    "updated_at": "datetime"
  },
  "SurveyResponse": {
    "id": "string (uuid)",
    "user_id": "string (uuid, FK to User)",
    "response": "object (structured JSON matching survey schema)",
    "created_at": "datetime",
    "updated_at": "datetime"
  },
  "Assessment": {
    "id": "string (uuid)",
    "user_id": "string (uuid, FK to User)",
    "survey_response_id": "string (uuid, FK to SurveyResponse)",
    "assessment": "string",
    "approved": "boolean",
    "revision_of": "string (uuid, FK to Assessment, nullable)",
    "created_at": "datetime",
    "updated_at": "datetime"
  },
  "Exercise": {
    "exercise_id": "string (primary key)",
    "name": "string",
    "image_url": "string (URL)",
    "equipments": "string[]",
    "body_parts": "string[]",
    "exercise_type": "string",
    "target_muscles": "string[]",
    "secondary_muscles": "string[]",
    "video_url": "string (URL)",
    "keywords": "string[]",
    "overview": "string",
    "instructions": "string[]",
    "exercise_tips": "string[]",
    "variations": "string[]",
    "related_exercise_ids": "string[]"
  },
  "ExerciseProgram": {
    "id": "string (uuid)",
    "user_id": "string (uuid, FK to User)",
    "assessment_id": "string (uuid, FK to Assessment)",
    "status": "string (draft, approved)",
    "approved_at": "datetime",
    "revision": "integer",
    "program_json": "object (structured program data)",
    "start_date": "date",
    "scheduling_preferences": "object",
    "last_scheduled_at": "datetime",
    "created_at": "datetime",
    "updated_at": "datetime"
  },
  "Session": {
    "id": "string (uuid)",
    "program_id": "string (uuid, FK to ExerciseProgram)",
    "week": "integer",
    "session_number": "integer",
    "notes": "string",
    "created_at": "datetime",
    "updated_at": "datetime"
  },
  "ChatMessage": {
    "id": "string (uuid)",
    "user_id": "string (uuid, FK to User)",
    "message": "string",
    "agent_type": "string (assessment, program, schedule)",
    "response": "string",
    "timestamp": "datetime"
  }
}